  "repository": {
    "type": "git",
    "url": "git+https://github.com/enjikaka/gulp-inline-style.git"
  },
  "keywords": [
    "gulp",
    "plugin",
    "css",
    "style",
    "inline"
  ],
  "author": "Jeremy Karlsson <jeremy@barlingshult.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enjikaka/gulp-inline-style/issues"
  },
  "homepage": "https://github.com/enjikaka/gulp-inline-style#readme",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/vinyl": "^2.0.12",
    "vitest": "^3.2.4",
    "vinyl": "^3.0.0"
  },
  "dependencies": {
    "plugin-error": "^2.0.1"
  }
}
