{
  "name": "gulp-inline-style",
  "version": "0.0.1",
  "description": "Gulp plugin to inline links to stylesheets.",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
